-        // change group and user
-        setGroup();
-        setUser();
-
-        initLogging();
-        workerProcess(filedes);
-      }
-      else
-      {
-        initWorkerProcess();
-        do
-        {
-          int filedes_monitor[2];
-
-          if (pipe(filedes_monitor) != 0)
-            throw std::runtime_error(
-              std::string("error in pipe(int[2]): ") + strerror(errno));
