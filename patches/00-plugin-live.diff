-      pf = propertyfilename.getValue();
-    else
-      pf = config.getValue("PropertyFile");
-
-    if (pf.empty())
-      log_init();
-    else
-    {
-      struct stat properties_stat;
-      if (stat(pf.c_str(), &properties_stat) != 0)
